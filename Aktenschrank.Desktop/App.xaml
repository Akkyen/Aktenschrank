<Application x:Class="Aktenschrank.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Aktenschrank"
             xmlns:valueConverters="clr-namespace:Aktenschrank.Desktop.ValueConverters"
             StartupUri="/View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <valueConverters:NullToBooleanConverter x:Key="NullToBoolConverter"/>
            <valueConverters:WidthMinusConstantConverter x:Key="WidthMinusConstantConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
